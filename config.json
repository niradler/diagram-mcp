{
    "mcpServers": {
        "diagram-mcp-stdio": {
            "command": "npx",
            "args": [
                "-y",
                "diagram-mcp"
            ],
            "env": {
                "PORT": "8099",
                "TRANSPORT_TYPE": "stdio"
            }
        },
        "diagram-mcp": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "http://localhost:8098/mcp"
            ]
        }
    }
}